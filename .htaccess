RewriteCond %{HTTP_HOST} ^www\.planning\.uds\.ub\.gov\.mn$
RewriteRule ^public_html\/uma\.uda\.ub\.gov\.mn$ "http\:\/\/uds\.ub\.gov\.mn\/" [R=301,L]

